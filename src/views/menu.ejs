  <body>
    <ol id="list" style="list-style: none">
      <table>
        <tr>
          <th>â„–</th>
          <th>Name</th>
          <th>Category</th>
          <th>go</th>
        </tr>
        <% newArr.map(el => { %>
        <li>
          <tr>
            <td><%= el.id %></td>
            <td style="display: flex; align-items: center">
              <img src="<%= el.image %> " style="width: 50px; height: 50px" alt=""/>
              <%= el.name %>
            </td>
            <td><%= el.category %></td>
            <td>
              <form action="/food" method="get" enctype="application/x-www-form-urlencoded" >
                <button data-id="<%= el.id %>" class="go-btn">go</button>
              </form>
            </td>
          </tr>
        </li>
        <% }) %>
      </table>
    </ol>

    <script>
      const list = document.querySelector("#list");
      list.addEventListener("click", (evt) => {
        let res_id;
        evt.target.dataset.id ? (res_id = evt.target.dataset.id) : "";
        document.cookie = "res_id" + "=" + (res_id || "");
      });
    </script>
  </body>
